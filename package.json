{
  "name": "campus-aar",
  "version": "0.1.0",
  "description": "Video analysis",
  "main": "server.js",
  "scripts": {
    "build:dev": "webpack -d --devtool cheap-eval-source-map",
    "build:prod": "NODE_ENV=production DEBUG=ERROR webpack -p --progress --colors",
    "build:vocab": "NODE_ENV=production webpack -p --progress --colors --config webpack/webpack.vocab.js",
    "dev": "NODE_ENV=development SERVER_PORT=3000 DEBUG='arma:*,arma-todo:*' node server.js",
    "dev-api": "NODE_ENV=development SERVER_PORT=3001 DEBUG='arma:*,arma-todo:*' SERVER_MODE=api nodemon -e js --watch app.js --watch modules --watch utils server.js",
    "start": "NODE_ENV=production SERVER_PORT=3000 node --harmony server.js",
    "test": "casperjs test tests/test-pages.js tests/test-search.js",
    "test-server": "DEBUG='async,app,arma:*' jasmine",
    "watch-test-server": "DEBUG='async,app,arma:*' nodemon -e js --watch app.js --watch modules --watch utils ./node_modules/.bin/jasmine",
    "test:pre-production": "casperjs --host='http://preprod.campus-aar.using.ovh' test tests/test-pages.js tests/test-search.js",
    "test:ci": "casperjs --host='http://preprod.campus-aar.using.ovh' test tests/test-pages.js tests/test-search.js --xunit=casper.xml"
  },
